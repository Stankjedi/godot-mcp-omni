                "position": { "$type": "Vector3", "x": 0, "y": 8, "z": 22 },
                "rotation_degrees": {
                  "$type": "Vector3",
                  "x": -15,
                  "y": 0,
                  "z": 0
                },
                "current": true
              }
            }
          },
          {
            "operation": "add_node",
            "params": {
              "scenePath": "res://scenes/solar_system/SolarSystem3D.tscn",
              "parentNodePath": "root",
              "nodeType": "Node3D",
              "nodeName": "Orbit_Mercury"
            }
          },
          {
            "operation": "add_node",
            "params": {
              "scenePath": "res://scenes/solar_system/SolarSystem3D.tscn",
              "parentNodePath": "root/Orbit_Mercury",
              "nodeType": "MeshInstance3D",
              "nodeName": "Mercury",
              "properties": {
                "position": { "$type": "Vector3", "x": 3.5, "y": 0, "z": 0 },
                "mesh": {
                  "$resource": "SphereMesh",
                  "props": { "radius": 0.25 }
                }
              }
            }
          },
          {
            "operation": "add_node",
            "params": {
              "scenePath": "res://scenes/solar_system/SolarSystem3D.tscn",
              "parentNodePath": "root",
              "nodeType": "Node3D",
              "nodeName": "Orbit_Venus"
            }
          },
          {
            "operation": "add_node",
            "params": {
              "scenePath": "res://scenes/solar_system/SolarSystem3D.tscn",
              "parentNodePath": "root/Orbit_Venus",
              "nodeType": "MeshInstance3D",
              "nodeName": "Venus",
              "properties": {
                "position": { "$type": "Vector3", "x": 5.2, "y": 0, "z": 0 },
                "mesh": {
                  "$resource": "SphereMesh",
                  "props": { "radius": 0.4 }
                }
              }
            }
          },
          {
            "operation": "add_node",
            "params": {
              "scenePath": "res://scenes/solar_system/SolarSystem3D.tscn",
              "parentNodePath": "root",
              "nodeType": "Node3D",
              "nodeName": "Orbit_Earth"
            }
          },
          {
            "operation": "add_node",
            "params": {
              "scenePath": "res://scenes/solar_system/SolarSystem3D.tscn",
              "parentNodePath": "root/Orbit_Earth",
              "nodeType": "MeshInstance3D",
              "nodeName": "Earth",
              "properties": {
                "position": { "$type": "Vector3", "x": 7.0, "y": 0, "z": 0 },
                "mesh": {
                  "$resource": "SphereMesh",
                  "props": { "radius": 0.45 }
                }
              }
            }
          },
          {
            "operation": "add_node",
            "params": {
              "scenePath": "res://scenes/solar_system/SolarSystem3D.tscn",
              "parentNodePath": "root",
              "nodeType": "Node3D",
              "nodeName": "Orbit_Mars"
